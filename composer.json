{
  "name": "meita/wps",
  "description": "Saudi WPS payroll protection file generator with multi-bank support (Al Rajhi, Riyad, AlAhli, Alinma, ANB) for any PHP or Laravel project.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Meita\\Wps\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Meita\\Wps\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Eng. Mohamed A. Eita",
      "email": "maa1987@hotmail.com"
    }
  ],
  "keywords": [
    "wps",
    "payroll",
    "saudi",
    "bank",
    "alrajhi",
    "riyad",
    "alahli",
    "alinma",
    "anb",
    "laravel",
    "php"
  ],
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Meita\\Wps\\PayrollProtectionServiceProvider"
      ]
    }
  }
}